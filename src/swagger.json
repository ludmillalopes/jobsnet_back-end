{
  "openapi": "3.0.0",

  "info": {
    "title": "API do JobsNET",
    "description": "API do cadastro de candidatos do JobsNET",
    "version": "0.0.1"
  },

  "servers": [
    {
      "url": "http://localhost:5000",
      "description": "ambiente de desenvolvimento"
    }
  ],

  "paths": {
    "/register": {
      "post": {
        "summary": "Cadastro de candidato",
        "description": "Rota de cadastro de usuário, recebe: nome, cpf, nascimento, cep, logradouro, numero, complemento, bairro, cidade, estado, emailForm, celular, profissao e file.",
        "tags": ["candidato"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "nome": {
                    "type": "string"
                  },
                  "cpf": {
                    "type": "number"
                  },
                  "nascimento": {
                    "type": "string"
                  },
                  "cep": {
                    "type": "number"
                  },
                  "logradouro": {
                    "type": "string"
                  },
                  "numero": {
                    "type": "number"
                  },
                  "complemento": {
                    "type": "string"
                  },
                  "bairro": {
                    "type": "string"
                  },
                  "cidade": {
                    "type": "string"
                  },
                  "estado": {
                    "type": "string"
                  },
                  "emailForm": {
                    "type": "string"
                  },
                  "celular": {
                    "type": "number"
                  },
                  "profissao": {
                    "type": "string"
                  },
                  "file": {
                    "type": "string"
                  }
                }
              },
              "examples": {
                "candidato1": {
                  "value": {
                    "nome": "João Silva",
                    "cpf": "12345678912",
                    "nascimento": "01021987",
                    "cep": "123456789",
                    "logradouro": "Rua Tal",
                    "numero": "100",
                    "complemento": "30",
                    "bairro": "Cicrano",
                    "cidade": "Beltrano",
                    "estado": "Fulano",
                    "emailForm": "email@email.com",
                    "celular": "99999999999",
                    "profissao": "Designer",
                    "file": "arquivo"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/signUp": {
      "post": {
        "summary": "Cadastro de usuário",
        "description": "Rota de cadastro de usuário, recebe: email e password.",
        "tags": ["user"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {                  
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              },
              "examples": {
                "user1": {
                  "value": {
                    "email": "email@email.com",
                    "password": "Senha123"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}